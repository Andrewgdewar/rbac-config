{
    "roles": [
      {
        "name": "Repositories administrator",
        "description": "Perform any available operation against any repositories resource.",
        "system": true,
        "platform_default": false,
        "admin_default": true,
        "version": 2,
        "access": [
          {
            "permission": "content-sources:repositories:read"
          },
          {
            "permission": "content-sources:repositories:write"
          },
          {
            "permission": "content-sources:repositories:upload"
          }
        ]
      },
      {
        "name": "Repositories viewer",
        "description": "Perform read only operations against repositories resources.",
        "system": true,
        "platform_default": true,
        "version": 2,
        "access": [
          {
            "permission": "content-sources:repositories:read"
          }
        ]
      },
      {
        "name": "Repositories uploader",
        "description": "Upload content to repositories.",
        "system": true,
        "platform_default": false,
        "version": 1,
        "access": [
          {
            "permission": "content-sources:repositories:upload"
          }
        ]
      },
      {
        "name": "Content Template administrator",
        "description": "Perform any available operation against any Content Template resource.",
        "system": true,
        "platform_default": false,
        "admin_default": true,
        "version": 2,
        "access": [
          {
            "permission": "content-sources:repositories:read"
          },
          {
            "permission": "content-sources:templates:read"
          },
          {
            "permission": "content-sources:templates:write"
          }
        ]
      },
      {
        "name": "Content Template viewer",
        "description": "Perform read only operations against Content Template resources.",
        "system": true,
        "platform_default": true,
        "version": 2,
        "access": [
          {
            "permission": "content-sources:repositories:read"
          },
          {
            "permission": "content-sources:templates:read"
          }
        ]
      }
    ]
  }
