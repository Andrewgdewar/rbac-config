{
  "roles": [
    {
      "name": "Drift analysis administrator",
      "description": "Perform any available operation on any Drift analysis resource.",
      "system": true,
      "admin_default": true,
      "version": 14,
      "access": [
        {
          "permission": "drift:comparisons:read"
        },
        {
          "permission": "drift:baselines:read"
        },
        {
          "permission": "drift:baselines:write"
        },
        {
          "permission": "drift:historical-system-profiles:read"
        },
        {
          "permission": "drift:notifications:read"
        },
        {
          "permission": "drift:notifications:write"
        },
        {
          "permission": "drift:*:*"
        }
      ]
    },
    {
      "name": "Drift viewer",
      "description": "Perform read-only operation on Drift analysis resources.",
      "system": true,
      "platform_default": true,
      "version": 5,
      "access": [
        {
          "permission": "drift:comparisons:read"
        },
        {
          "permission": "drift:baselines:read"
        },
        {
          "permission": "drift:historical-system-profiles:read"
        },
        {
          "permission": "drift:notifications:read"
        }
      ]
    }
  ]
}
