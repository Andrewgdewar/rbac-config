{
  "roles": [
    {
      "name": "Inventory administrator",
      "description": "Perform any available operation on any Inventory resource.",
      "system": true,
      "platform_default": false,
      "admin_default": false,
      "version": 4,
      "access": [
        {
          "permission": "inventory:*:*"
        }
      ]
    },
    {
      "name": "Inventory Hosts Administrator",
      "display_name": "Inventory Hosts administrator",
      "description": "Perform read and update operations on Inventory Hosts data.",
      "system": true,
      "platform_default": true,
      "admin_default": true,
      "version": 4,
      "access": [
        {
          "permission": "inventory:hosts:write"
        },
        {
          "permission": "inventory:hosts:read"
        }
      ]
    },
    {
      "name": "Inventory Hosts Viewer",
      "display_name": "Inventory Hosts viewer",
      "description": "Perform read operations on Inventory Hosts data.",
      "system": true,
      "platform_default": false,
      "admin_default": false,
      "version": 4,
      "access": [
        {
          "permission": "inventory:hosts:read"
        }
      ]
    },
    {
      "name": "Inventory Groups Administrator",
      "display_name": "Workspace administrator",
      "description": "Perform read and update operations on Workspace data.",
      "system": true,
      "platform_default": false,
      "admin_default": true,
      "version": 5,
      "access": [
        {
          "permission": "inventory:groups:write"
        },
        {
          "permission": "inventory:groups:read"
        }
      ]
    },
    {
      "name": "Inventory Groups Viewer",
      "display_name": "Workspace viewer",
      "description": "Perform read operations on Workspace data.",
      "system": true,
      "platform_default": false,
      "admin_default": false,
      "version": 5,
      "access": [
        {
          "permission": "inventory:groups:read"
        }
      ]
    }
  ]
}
